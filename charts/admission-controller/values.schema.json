{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "commonNodeSelector": {
            "type": "object"
        },
        "commonTolerations": {
            "type": "array"
        },
        "config": {
            "type": "object",
            "properties": {
                "admission": {
                    "type": "object",
                    "properties": {
                        "glob": {
                            "type": "array"
                        }
                    }
                },
                "attest": {
                    "type": "object",
                    "properties": {
                        "cocosign": {
                            "type": "object",
                            "properties": {
                                "storer": {
                                    "type": "object",
                                    "properties": {
                                        "OCI": {
                                            "type": "object",
                                            "properties": {
                                                "enable": {
                                                    "type": "boolean"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        },
                        "default": {
                            "type": "string"
                        }
                    }
                },
                "context": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        }
                    }
                },
                "verify": {
                    "type": "object",
                    "properties": {
                        "input-format": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "scribe": {
            "type": "object",
            "properties": {
                "auth": {
                    "type": "object",
                    "properties": {
                        "client_id": {
                            "type": "string"
                        },
                        "client_secret": {
                            "type": "string"
                        }
                    }
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "enable": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "serviceMonitor": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "webhook": {
            "type": "object",
            "properties": {
                "env": {
                    "type": "object"
                },
                "extraArgs": {
                    "type": "object",
                    "properties": {
                        "structured": {
                            "type": "boolean"
                        },
                        "verbose": {
                            "type": "integer"
                        }
                    }
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "version": {
                            "type": "string"
                        }
                    }
                },
                "name": {
                    "type": "string"
                },
                "podSecurityContext": {
                    "type": "object",
                    "properties": {
                        "allowPrivilegeEscalation": {
                            "type": "boolean"
                        },
                        "capabilities": {
                            "type": "object",
                            "properties": {
                                "drop": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "readOnlyRootFilesystem": {
                            "type": "boolean"
                        },
                        "runAsUser": {
                            "type": "integer"
                        }
                    }
                },
                "replicaCount": {
                    "type": "integer"
                },
                "secretName": {
                    "type": "string"
                },
                "securityContext": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "runAsUser": {
                            "type": "integer"
                        }
                    }
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "port": {
                            "type": "integer"
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                },
                "serviceAccount": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        }
                    }
                },
                "webhookName": {
                    "type": "string"
                }
            }
        }
    }
}
